@model Depot.App.ViewModels.ProdutoViewModel

@{
    ViewData["Title"] = "Atualizar Produto";
}

<div class="modal-header">
    <h4 class="modal-title">@ViewData["Title"]</h4>
    <button type="button" class="close" data-dismiss="modal">
        <span aria-hidden="true">x</span><span class="sr-only">Fechar</span>
    </button>
</div>
<form asp-action="AtualizarProduto">
    <div class="modal-body">
        <input type="hidden" asp-for="Id" />
       

        <partial name="_Produto" />

        <div class="modal-footer">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Atualizar Endereço" class="btn btn-success" />
                <input type="button" class="btn btn-info" value="Fechar" data-dismiss="modal" />
            </div>
        </div>
    </div>
</form>
<script>
    BuscaCep();
</script>